new job:        job_info = {"type": "NEW_JOB", "job_name": job_name, "input_dir": input_dir, "output_dir": output_dir}
map task:       task_info = {"type": "MAP_TASK", "map_task_id": job_name + "_m_" + file, "map_task_file": file}
map task end:   map_task_end_info = {"type": "MAP_TASK_END"}


client related:
new job:        job_information = {"type": "NEW_JOB", "job_name": "WordCount", "job_fs_path": "wordcount"}
feedback:       feedback_info = {"type": "FEEDBACK", "status": "ERROR", "message": "checking directories failed"}